    "# Beautiful soup, requests and pandas (pandas used here for csv wrangling)\n",
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "import re # needed for regular expression to strip out grammar\n",
